---
interface Props {
  width?: number;
  height?: number;
  aClassName?: string;
  viewId?: string;
}

const { width, height, aClassName, viewId } = Astro.props;
const { href } = Astro.url;
---

<a href={href} class={aClassName} data-view-id={viewId}>
  <img
    src="/logo_no_background.png"
    alt="logo"
    width={width ?? "120"}
    height={height ?? 120}
  />
</a>
